<h1>Here are the Blog posts</h1>
<h2><%=@blog.title%></h2>
<p><%=@blog.description%></p>


<%= form_with model: @blog, local: true, method: "delete" do |form|%>
    <% if current_user.id == @blog.user_id%>
        <%= submit_tag "delete"%>
        <%= link_to "edit", edit_blog_path(@blog.id)%>
    <% end %>
<% end %>


